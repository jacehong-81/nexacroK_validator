{
    "extends": "./tsconfig.base.json",
    "compilerOptions": {
        "target": "esnext",
        "module": "ESNext",
        "lib": [
            "ESNext",
            "DOM",
            "DOM.Iterable",
            //"esnext.decorators"
        ],
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "strict": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "useDefineForClassFields": false,
        "noImplicitOverride": true,
    },
}